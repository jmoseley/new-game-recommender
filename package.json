{
  "name": "game-recommending-bot",
  "version": "0.1.0",
  "description": "A bot that post about Steam announcements",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmoseley/new-game-recommender.git"
  },
  "author": "Jeremy Moseley",
  "license": "MIT",
  "scripts": {
    "build": "npm run tslint && webpack",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "async": "^2.5.0",
    "aws-sdk": "^2.135.0",
    "decimal.js": "^7.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "node-promisify": "^1.2.0",
    "raw.js": "^0.6.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "steam-api": "^1.1.3",
    "steam-store": "^0.4.1",
    "ts-node": "^3.3.0",
    "uuid": "^3.1.0",
    "xmljson": "^0.2.0",
    "yarn": "^1.2.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.44",
    "@types/decimal.js": "^0.0.31",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.77",
    "@types/moment": "^2.13.0",
    "@types/request-promise-native": "^1.0.7",
    "@types/uuid": "^3.4.2",
    "jest": "^19.0.2",
    "serverless-dynamodb-local": "^0.2.26",
    "serverless-kms-secrets": "^1.0.2",
    "serverless-offline": "^3.16.0",
    "serverless-offline-scheduler": "^0.3.3",
    "serverless-webpack": "^3.1.2",
    "source-map-support": "^0.5.0",
    "ts-jest": "^19.0.8",
    "ts-loader": "^3.0.2",
    "tslint": "^5.0.0",
    "typescript": "^2.4.0",
    "webpack": "^3.7.1"
  }
}
